@model ProyectoTransportesAndes.Models.Vehiculo

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Modificar vehiculo</h3>
<div>
    <a asp-action="Index">Back to List</a>
</div>
<hr />
<div class="row">
